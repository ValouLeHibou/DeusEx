{

  "manifest_version": 2,
  "name": "Deus EX - The Explorer's Explorer",
  "version": "1.0",

  "description": "Check for new URL in the google research",
  "homepage_url": "https://github.com",

  "icons": {
    "32": "icons/beasts-32.png",
    "48": "icons/beasts-48.png",
    "96": "icons/beasts-96.png"
  },

  "permissions": [
    "activeTab"
  ],

    "browser_action": {
    "default_icon": "icons/beasts-32.png",
    "default_title": "DeusEx",
    "default_popup": "popup/dashboard.html"
  },

  "background": {
  "scripts": ["deusex.js"]
  }
}